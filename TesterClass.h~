#ifndef TESTERCLASS_H
#define TESTERCLASS_H

#include <iostream>
using namespace std;
#include <string>
#include <cstdlib>

class TesterClass {

	public:
		int num;

	public:

		TesterClass();

		TesterClass(int);

		~TesterClass();

		bool operator==(const TesterClass&);

		bool operator<(const TesterClass&);

		bool operator>(const TesterClass&);

		friend std::ostream& operator<<(std::ostream& o, const TesterClass&);

};

#endif
